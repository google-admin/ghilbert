term I : term;
kind wff;
term = x y: wff;

axiom AS: = (`(`(`Sx)y)z) (`(`xz)(`yz));
axiom AK: = (`(`Kx)y) x;
axiom fveq1i (= x y): = (`xz) (`yz);
axiom fveq2i (= x y): = (`zx) (`zy);
