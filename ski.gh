axiom eta (= (`xz) (`yz)): = x y;

theorem AI: = (`(`(`SK)K)x) x ::(
    AS
    [= (`(`(`SK)K)x) (`(`Kx)(`Kx))]
    eqtri _ (
        AK
        [= (`(`Kx)(`Kx)) x]
    )
