syntax {
    infixl ` 90;
    infixl = 50;
}
kind term;
var x y z: term;
term ` term term: term;
term S : term;
term K : term;
term I : term;
kind wff;
term = term term: wff;

axiom AS: S`x`y`z = (x`z)`(y`z);
axiom AK: K`x`y = x;
axiom fveq1i (x = y): x`z = y`z;
axiom fveq2i (x = y): z`x = z`y;
axiom eqtri (x = y) (y = z): x = z;
axiom eta (x`z = y`z): x = y;

theorem AI: S`K`K`x = x ::(
    AS
    [S`K`K`x = (K`x)`(K`x)]
    eqtri _ (
        AK
        [(K`x)`(K`x) = x]
    )
)
